{
  "name": "@maxfield/node-comet2",
  "version": "0.1.0",
  "description": "COMET2 CLI",
  "main": "dist/src/app.js",
  "bin": {
    "node-comet2": "dist/src/app.js"
  },
  "preferGlobal": true,
  "author": "Maxfield Walker",
  "license": "MIT",
  "scripts": {
    "compile": "node ./node_modules/typescript/bin/tsc -p .",
    "watch": "node ./node_modules/typescript/bin/tsc -w -p .",
    "test": "npm run compile && mocha",
    "test:coverage": "npm run compile && istanbul cover ./node_modules/mocha/bin/_mocha -- --no-timeouts && remap-istanbul --input ./coverage/coverage.json --output ./coverage/lcov.ts.info --type lcovonly",
    "prepublish": "npm run compile"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.54",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "coveralls": "^2.12.0",
    "mocha": "^2.3.3",
    "remap-istanbul": "^0.9.1",
    "typescript": "^2.2.1"
  },
  "dependencies": {
    "@maxfield/node-casl2-comet2-common": "^0.0.5",
    "@maxfield/node-casl2-comet2-core-common": "0.1.0",
    "@maxfield/node-comet2-core": "^0.1.0",
    "colors": "^1.1.2",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "readline-sync": "^1.4.6"
  }
}